#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/description
dc:description rdf:type owl:AnnotationProperty .


###  http://schema.org/affiliation
schema:affiliation rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
<http://www.w3.org/2004/02/skos/core#altLabel> rdf:type owl:AnnotationProperty .


###  https://w3id.org/i40/rami#hasRAMIHierarchyLevel
rami:hasRAMIHierarchyLevel rdf:type owl:AnnotationProperty .


###  https://w3id.org/i40/rami#hasRAMILifeCycleLayer
rami:hasRAMILifeCycleLayer rdf:type owl:AnnotationProperty .


###  https://w3id.org/i40/sto/standards#domainName
<https://w3id.org/i40/sto/standards#domainName> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/license
dc:license rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/made
foaf:made rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/maker
foaf:maker rdf:type owl:ObjectProperty .


###  https://w3id.org/i40/sto#developed
sto:developed rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf foaf:made ;
              owl:inverseOf sto:developer ;
              rdfs:domain sto:SDO ;
              rdfs:range sto:Standard ;
              dc:description "A standard developed by this organization"@en ;
              rdfs:label "Published" .


###  https://w3id.org/i40/sto#developer
sto:developer rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf foaf:maker ;
              rdfs:domain sto:Standard ;
              rdfs:range sto:SDO ;
              dc:description "The organization that developed this Standard"@en ;
              rdfs:label "Developer" .


###  https://w3id.org/i40/sto#hasDBpediaResource
sto:hasDBpediaResource rdf:type owl:ObjectProperty ;
                       dc:description "Instance's DBpedia Resource"@en ;
                       rdfs:label "DBpediaresource"@en .


###  https://w3id.org/i40/sto#hasOfficialResource
sto:hasOfficialResource rdf:type owl:ObjectProperty ;
                        dc:description "Instance's Official Website"@en ;
                        rdfs:label "hasOfficialResource"@en .


###  https://w3id.org/i40/sto#hasTag
sto:hasTag rdf:type owl:ObjectProperty ;
           rdfs:domain sto:Standard ;
           rdfs:range muto:Tag .


###  https://w3id.org/i40/sto#hasWikidataEntity
sto:hasWikidataEntity rdf:type owl:ObjectProperty ;
                      dc:description "Instance's Wikidata Entry"@en ;
                      rdfs:label "hasWikidataEntity"@en .


###  https://w3id.org/i40/sto#hasWikipediaArticle
sto:hasWikipediaArticle rdf:type owl:ObjectProperty .


###  https://w3id.org/i40/sto#license
sto:license rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf dc:license ;
            rdfs:domain sto:Standard .


###  https://w3id.org/i40/sto#motivation
sto:motivation rdf:type owl:ObjectProperty ;
               rdfs:domain sto:Standard ;
               rdfs:range deo:Motivation .


###  https://w3id.org/i40/sto#published
sto:published rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf foaf:made ;
              owl:inverseOf sto:publisher ;
              rdfs:domain sto:SDO ;
              rdfs:range sto:Standard ;
              dc:description "A standard published by this organization"@en ;
              rdfs:label "Published" .


###  https://w3id.org/i40/sto#publisher
sto:publisher rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf foaf:maker ;
              rdfs:domain sto:Standard ;
              rdfs:range sto:SDO ;
              dc:description "The organization that published this Standard"@en ;
              rdfs:label "Publisher" .


###  https://w3id.org/i40/sto#ramiHierarchyLevel
sto:ramiHierarchyLevel rdf:type owl:ObjectProperty ;
                       rdfs:domain sto:Standard ;
                       rdfs:range rami:RAMIHierarchyLevel ;
                       dc:description "associates the standards with the RAMI level in which they are applicable"@en ;
                       rdfs:label "RAMIHierarchyLevel"@en .


###  https://w3id.org/i40/sto#relatedTo
sto:relatedTo rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain sto:Standard ;
              rdfs:range sto:Standard ;
              dc:description "Both Standards are related"@en ;
              rdfs:label "isRelatedto"@en .


###  https://w3id.org/i40/sto#scope
sto:scope rdf:type owl:ObjectProperty ;
          rdfs:domain sto:Standard ;
          rdfs:range sto:IndustryDomain .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/i40/sto#abbreviation
sto:abbreviation rdf:type owl:DatatypeProperty ;
                 rdfs:domain sto:SDO ;
                 dc:description "Abbreviated  name of the organization"@en ;
                 rdfs:label "hasAbbreviatedName"@en .


###  https://w3id.org/i40/sto#domainName
sto:domainName rdf:type owl:DatatypeProperty ;
               rdfs:domain sto:IndustryDomain ;
               dc:description "Name of the Industry Domain"@en ;
               rdfs:label "hasDomainName"@en .


###  https://w3id.org/i40/sto#formationDate
sto:formationDate rdf:type owl:DatatypeProperty ;
                  rdfs:domain sto:SDO ;
                  rdfs:range xsd:date ;
                  dc:description "Abbreviated  name of the organization"@en ;
                  rdfs:label "hasFormationDate"@en .


###  https://w3id.org/i40/sto#hasImplementationFormat
sto:hasImplementationFormat rdf:type owl:DatatypeProperty ;
                            rdfs:domain sto:Standard ;
                            dc:description "Describes the name of the implementation or serialization format of the standard, e.g., XML, Document, binary, batch"@en ;
                            rdfs:label "hasImplementationFormat"@en .


###  https://w3id.org/i40/sto#hasOntology
sto:hasOntology rdf:type owl:DatatypeProperty ;
                rdfs:domain sto:Standard ;
                rdfs:range xsd:anyURI ;
                rdfs:comment "Point to the URL of the Ontology representation of the Standard"@en ;
                rdfs:label "hasOntology"@en .


###  https://w3id.org/i40/sto#name
sto:name rdf:type owl:DatatypeProperty ;
         rdfs:domain sto:SDO ;
         dc:description "Name of the Organization"@en ;
         rdfs:label "Name" .


###  https://w3id.org/i40/sto#norm
sto:norm rdf:type owl:DatatypeProperty ;
         rdfs:domain sto:Standard ;
         dc:description "Norm under which the standard is published"@en ;
         rdfs:label "Norm" .


#################################################################
#    Classes
#################################################################

###  http://purl.org/muto/core#Tag
muto:Tag rdf:type owl:Class .


###  http://purl.org/spar/deo/Motivation
deo:Motivation rdf:type owl:Class .


###  http://schema.org/Person
schema:Person rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Document
foaf:Document rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Organization
foaf:Organization rdf:type owl:Class .


###  https://w3id.org/i40/rami#RAMIHierarchyLevel
rami:RAMIHierarchyLevel rdf:type owl:Class .


###  https://w3id.org/i40/sto#IndustryDomain
sto:IndustryDomain rdf:type owl:Class ;
                   dc:description "Represents the possible industry domains"@en ;
                   rdfs:label "IndustryDomain"@en .


###  https://w3id.org/i40/sto#SDO
sto:SDO rdf:type owl:Class ;
        rdfs:subClassOf foaf:Organization ;
        owl:disjointWith sto:Standard ;
        dc:description "Define the standards organizations that develop and publish industry standards"@en ;
        rdfs:label "StandardDevelopingOrganization"@en .


###  https://w3id.org/i40/sto#Standard
sto:Standard rdf:type owl:Class ;
             rdfs:subClassOf foaf:Document ;
             dc:description "Standards Class"@en ;
             rdfs:label "Standard" .


