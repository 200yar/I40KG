    </owl:ObjectProperty>
    


    <!-- http://xmlns.com/foaf/0.1/maker -->

    <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/maker"/>
    


    <!-- https://w3id.org/i40/sto#belongsTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#belongsTo">
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#StandardOrganization"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology#Country"/>
        <rdfs:comment xml:lang="en">A Standard Organization that belongs to the country</rdfs:comment>
        <rdfs:label xml:lang="en">belongs To</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#developer -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#developer">
        <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/maker"/>
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <rdfs:range rdf:resource="https://w3id.org/i40/sto#StandardOrganization"/>
        <rdfs:comment xml:lang="en">The organization that developed this Standard</rdfs:comment>
        <rdfs:label xml:lang="en">developer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#hasClassification -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#hasClassification">
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <rdfs:range rdf:resource="https://w3id.org/i40/sto#StandardClassification"/>
        <rdfs:comment xml:lang="en">An Standard is isCategorizedBy a given Categorization Area which is connected to a specific Standarization Initiative</rdfs:comment>
        <rdfs:label xml:lang="en">has Classification</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#hasDomain -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#hasDomain">
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <rdfs:range rdf:resource="https://w3id.org/i40/sto#Domain"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#hasHeadquaterIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#hasHeadquaterIn">
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#StandardOrganization"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology#Country"/>
        <rdfs:comment xml:lang="en">Denotes the countries in which the Headquarter of the organization who publishes the Standart</rdfs:comment>
        <rdfs:label xml:lang="en">has Headquater In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#hasTag -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#hasTag">
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <rdfs:range rdf:resource="http://purl.org/muto/core#Tag"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#isDescribedin -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#isDescribedin">
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#StandardClassification"/>
        <rdfs:range rdf:resource="https://w3id.org/i40/sto#StandarizationInitiative"/>
        <rdfs:comment xml:lang="en">Links the Standarization Initiatives with the Classification of Standards</rdfs:comment>
        <rdfs:label xml:lang="en">is Described in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#isImplementationOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#isImplementationOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/i40/sto#relatedTo"/>
        <rdfs:comment xml:lang="en">Represent the relation that connects two standards when standard A is the implementation of standard B, e.g., B2MML isImplementationOf ISA-95.</rdfs:comment>
        <rdfs:label xml:lang="en">is Implementation Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#isPartOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#isPartOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/i40/sto#relatedTo"/>
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <rdfs:range rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <rdfs:comment xml:lang="en">Represents Standards that are part of other Standards</rdfs:comment>
        <rdfs:label xml:lang="en">is Part Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#motivation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#motivation">
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <rdfs:range rdf:resource="http://purl.org/spar/deo/Motivation"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#publisher -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#publisher">
        <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/maker"/>
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <rdfs:range rdf:resource="https://w3id.org/i40/sto#StandardOrganization"/>
        <rdfs:comment xml:lang="en">The organization that published this Standard</rdfs:comment>
        <rdfs:label xml:lang="en">publisher</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/i40/sto#relatedTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/i40/sto#relatedTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <rdfs:range rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <rdfs:comment xml:lang="en">Represent any type of relation that connects two standards.</rdfs:comment>
        <rdfs:label xml:lang="en">related To</rdfs:label>
        <skos:altLabel xml:lang="en">has Associated Standard</skos:altLabel>
    </owl:ObjectProperty>
    


