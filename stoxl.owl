    <!-- https://w3id.org/i40/sto#ETSI_DECT -->

    <owl:NamedIndividual rdf:about="https://w3id.org/i40/sto#ETSI_DECT">
        <rdf:type rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <sto:developer rdf:resource="https://w3id.org/i40/sto#ETSI"/>
