    <!-- https://w3id.org/i40/rami#AdminShellSubmodel -->

    <owl:Class rdf:about="https://w3id.org/i40/rami#AdminShellSubmodel">
        <rdfs:subClassOf rdf:resource="https://w3id.org/i40/sto#StandardClassification"/>
        <rdfs:comment xml:lang="en">Describes the Administration Shell concept to organize/classify standards into submodels</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.plattform-i40.de/I40/Redaktion/EN/Downloads/Publikation/structure-of-the-administration-shell.pdf?__blob=publicationFile&amp;v=7"/>
        <rdfs:label xml:lang="en">Admin Shell Sub Model</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/i40/rami#RAMIHierarchyLevel -->

    <owl:Class rdf:about="https://w3id.org/i40/rami#RAMIHierarchyLevel">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#ConnectedWorld"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#ControlDevice"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#Enterprise"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#FieldDevice"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#Product"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#Station"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#WorkCenter"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/i40/sto#StandardClassification"/>
        <rdfs:label xml:lang="en">RAMI Hierarchy Level</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/i40/rami#RAMIITLayer -->

    <owl:Class rdf:about="https://w3id.org/i40/rami#RAMIITLayer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#Asset"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#Business"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#Communication"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#Functional"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#Information"/>
                    <rdf:Description rdf:about="https://w3id.org/i40/rami#Integration"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/i40/sto#StandardClassification"/>
        <rdfs:label xml:lang="en">RAMI IT Layer</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/i40/rami#RAMILifeCycleLayer -->

    <owl:Class rdf:about="https://w3id.org/i40/rami#RAMILifeCycleLayer">
        <rdfs:subClassOf rdf:resource="https://w3id.org/i40/sto#StandardClassification"/>
